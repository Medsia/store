@{
    ViewData["Title"] = "Инфо страницы";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>@ViewData["Title"]</h3>

<table class="table">
        <tr>
            <td>
                <form action="@Url.Action("ContactsInfo", "Admin")" method="post">
                    <button class="btn btn-outline-info" type="submit">Контакты</button>
                </form>
            </td>
        </tr>
        <tr>
            <td>
                <form action="@Url.Action("PaymentInfo", "Admin")" method="post">
                    <button class="btn btn-outline-info" type="submit">Оплата</button>
                </form>
            </td>
        </tr>
        <tr>
            <td>
                <form action="@Url.Action("DeliveryInfo", "Admin")" method="post">
                    <button class="btn btn-outline-info" type="submit">Доставка</button>
                </form>
            </td>
        </tr>
        <tr>
            <td>
                <form action="@Url.Action("AboutInfo", "Admin")" method="post">
                    <button class="btn btn-outline-info" type="submit">О магазине</button>
                </form>
            </td>
        </tr>
</table>
