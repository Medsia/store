@model Store.Data.Content.ContactsSO

@{
    ViewData["Title"] = Model.Title;
}

<p class="info-title">@Model.Title</p>

<div class="info-location">
    <label class="info-label">Наш адрес</label>
    <p class="info-text">@Model.Location</p>
</div>

<div class="info-worktime">
    <label class="info-label">Время работы</label>
    <p class="info-text">@Model.Worktime</p>
</div>


<div class="info-numers">
    <label class="info-label">Телефон</label>
    @foreach(var item in Model.Numbers)
    {
        <p class="info-text">@item</p>
    }
</div>


<div class="info-additional">
    <label class="info-label">Дополнительная информация</label>
    <p class="info-text">@Model.Additional</p>
</div>